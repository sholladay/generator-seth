{
    "name": "<%= pkgName %>",
    "version": "0.0.0",
    "description": "<%= description %>",
    "homepage": "<%= repoUrl %>",
    "main": "index.js",
    "author": {
        "name": "<%= fullName %>",
        "url": "<%= website %>",
        "email": "<%= email %>"
    },
    "scripts": {
        "test": "xo && ava"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:<%= username %>/<%= pkgName %>.git"
    },
    "bugs": {
        "url": "<%= repoUrl %>/issues",
        "email": "<%= email %>"
    },
    "engines": {
        "node": ">=8"
    },
    "license": "MPL-2.0",<% if (cli) { %>
    "bin": "cli.js",<% } %>
    "files": [
        "index.js"<% if (cli) { %>,
        "cli.js"<% } %>
    ],<% if (cli) { %>
    "dependencies": {
        "meow": "^5.0.0"
    },<% } %>
    "devDependencies": {
        "ava": "^1.0.0-rc.2",
        "eslint-config-tidy": "^0.6.4",
        "xo": "^0.23.0"
    },
    "keywords": [
        <% if (cli) { %>"cli-app",
        "cli"<% } %>
    ],
    "xo": {
        "extend": "tidy"
    }
}
